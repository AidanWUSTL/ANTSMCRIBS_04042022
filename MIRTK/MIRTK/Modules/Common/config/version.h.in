#ifndef MIRTK_VersionInfo_H
#define MIRTK_VersionInfo_H


/// Major version number
#define MIRTK_VERSION_MAJOR @PROJECT_VERSION_MAJOR@

/// Minor version number
#define MIRTK_VERSION_MINOR @PROJECT_VERSION_MINOR@

/// Patch number
#define MIRTK_VERSION_PATCH @PROJECT_VERSION_PATCH@

/// Full version string (including 'rc', 'beta' or 'alpha' prefix)
